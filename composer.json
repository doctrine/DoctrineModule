{
    "name":        "doctrine/doctrine-module",
    "description": "Zend Framework 3 Module that provides Doctrine basic functionality required for ORM and ODM modules",
    "type":        "library",
    "license":     "MIT",
    "homepage":    "http://www.doctrine-project.org/",
    "keywords":    [
        "doctrine",
        "module",
        "zf3"
    ],
    "authors":     [
        {
            "name": "Kyle Spraggs",
            "email": "theman@spiffyjr.me",
            "homepage": "http://www.spiffyjr.me/"
        },
        {
            "name": "Marco Pivetta",
            "email": "ocramius@gmail.com",
            "homepage": "http://marco-pivetta.com/"
        },
        {
            "name": "Guilherme Blanco",
            "email": "guilhermeblanco@hotmail.com"
        },
        {
            "name": "MichaÃ«l Gallego",
            "email": "mic.gallego@gmail.com",
            "homepage": "http://www.michaelgallego.fr"
        }
    ],
    "require": {
        "php":                               "^5.6 || ^7.0",
        "doctrine/common":                   "^2.6",
        "doctrine/cache":                    "^1.6",
        "symfony/console":                   "^2.3 || ^3.0",
        "zendframework/zend-authentication": "^2.5.3",
        "zendframework/zend-cache":          "^2.7.1",
        "zendframework/zend-form":           "^2.9",
        "zendframework/zend-hydrator":       "^2.2.1",
        "zendframework/zend-mvc":            "^3.0.2",
        "zendframework/zend-mvc-console":    "^1.1.10",
        "zendframework/zend-paginator":      "^2.7",
        "zendframework/zend-servicemanager": "^3.1.0",
        "zendframework/zend-stdlib":         "^3.0.1",
        "zendframework/zend-validator":      "^2.8.1"
    },
    "require-dev": {
        "phpunit/phpunit":                  "^5.4",
        "squizlabs/php_codesniffer":        "^2.6.1",
        "zendframework/zend-modulemanager": "^2.7.2",
        "zendframework/zend-session":       "^2.7.3",
        "zendframework/zend-test":          "^3.0.1"
    },
    "suggest": {
        "doctrine/data-fixtures": "Data Fixtures if you want to generate test data or bootstrap data for your deployments"
    },
    "autoload": {
        "psr-4": {
            "DoctrineModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DoctrineModuleTest\\": "tests/"
        }
    },
    "bin": [
        "bin/doctrine-module"
    ],
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    }
}

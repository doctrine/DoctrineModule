            "email": "guilhermeblanco@hotmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "doctrine/common": "2.*",
        "doctrine/data-fixtures": "dev-master",
        "symfony/yaml": "2.0.*",
        "symfony/console": "2.0.*"
